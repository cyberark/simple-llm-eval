{
  "name": "local-report-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "npm run build:eval && npm run build:compare && npm run build:summary",
    "postbuild:eval": "node scripts/inject_placeholders_eval.cjs",
    "postbuild:compare": "node scripts/inject_placeholders_compare.cjs",
    "postbuild:summary": "node scripts/inject_placeholders_summary.cjs",
    "build:eval": "vite build --config vite.config.eval.js",
    "build:compare": "vite build --config vite.config.compare.js",
    "build:summary": "vite build --config vite.config.summary.js",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest",
    "test-no-watch": "vitest run"
  },
  "dependencies": {
    "@babel/generator": "^7.27.3",
    "@babel/parser": "^7.26.7",
    "@babel/traverse": "^7.26.7",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/material": "^6.4.3",
    "@mui/x-charts": "^7.26.0",
    "@tailwindcss/vite": "^4.0.1",
    "lucide-react": "^0.474.0",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "tailwindcss": "^4.0.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.17.0",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "globals": "^15.14.0",
    "jsdom": "^26.0.0",
    "postcss": "^8.5.1",
    "vite": "^6.0.5",
    "vite-plugin-singlefile": "^2.1.0",
    "vitest": "^3.2.4"
  }
}
